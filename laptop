keycode  34 = BackSpace BackSpace BackSpace BackSpace
keycode  62 = slash question slash question
keycode  22 = Delete Delete Delete Delete

remove Mod1 = Alt_L
remove Control = Control_L
keysym Control_L = Alt_L
keysym Alt_L = Control_L
add Mod1 = Alt_L
add Control = Control_L

remove Lock = Caps_Lock
keysym Escape = Caps_Lock
keysym Caps_Lock = Escape
add Lock = Caps_Lock
